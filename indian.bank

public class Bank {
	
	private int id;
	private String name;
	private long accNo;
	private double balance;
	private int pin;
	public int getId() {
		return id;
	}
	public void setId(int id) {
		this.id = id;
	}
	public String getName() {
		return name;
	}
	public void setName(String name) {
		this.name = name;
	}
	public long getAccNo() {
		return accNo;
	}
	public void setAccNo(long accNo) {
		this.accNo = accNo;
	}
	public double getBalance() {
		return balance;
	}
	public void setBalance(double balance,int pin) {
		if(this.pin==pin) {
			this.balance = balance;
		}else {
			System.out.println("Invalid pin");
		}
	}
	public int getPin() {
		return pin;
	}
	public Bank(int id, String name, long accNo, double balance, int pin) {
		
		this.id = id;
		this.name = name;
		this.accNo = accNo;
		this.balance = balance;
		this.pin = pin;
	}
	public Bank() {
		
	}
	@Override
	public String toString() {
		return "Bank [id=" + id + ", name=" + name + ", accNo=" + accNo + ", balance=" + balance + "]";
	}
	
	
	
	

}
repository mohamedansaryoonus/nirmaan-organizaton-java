day 19
package indian.bank;


public class Bank {
	
	private int id;
	private String name;
	private long accNo;
	private double balance;
	private int pin;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public long getAccNo() {
		return accNo;
	}
	public void setAccNo(long accNo) {
		this.accNo = accNo;
	}
	public double getBalance() {
		return balance;
	}
	public void setBalance(double balance,int pin) {
		if(this.pin==pin) {
			this.balance = balance;
		}else {
			System.out.println("Invalid pin");
		}
	}
	public int getPin() {
		return pin;
	}
	public Bank(int id, String name, long accNo, double balance, int pin) {
		
		this.id = id;
		this.name = name;
		this.accNo = accNo;
		this.balance = balance;
		this.pin = pin;
	}
	public Bank() {
		
	}
	@Override
	public String toString() {
		return "Bank [id=" + id + ", name=" + name + ", accNo=" + accNo + ", balance=" + balance + "]";
	}
	
	
	
	

}